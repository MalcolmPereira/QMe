	<table catalog="qme" name="CATEGORY"
		   class="com.malcolm.qme.springdata.entity.CategoryEntity">
	</table>
	<table catalog="qme" name="USER_CATEGORY"
		   class="com.malcolm.qme.springdata.entity.UserCategoryEntity">
	</table>
</hibernate-reverse-engineering>