	<table catalog="qme" name="USER_QUESTION_LIKES"
		   class="com.malcolm.qme.springdata.entity.UserQuestionLikesEntity">
	</table>
	<table catalog="qme" name="USER"
		   class="com.malcolm.qme.springdata.entity.UserEntity">
	</table>
	<table catalog="qme" name="USER_QUIZ"
		   class="com.malcolm.qme.springdata.entity.UserQuizEntity">
	</table>
	<table catalog="qme" name="USER_ROLES"
		   class="com.malcolm.qme.springdata.entity.UserRolesEntity">
