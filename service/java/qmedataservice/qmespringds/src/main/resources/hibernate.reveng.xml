<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<!--suppress ALL -->
<hibernate-reverse-engineering>
	<type-mapping>
		<sql-type jdbc-type="BIGINT"  hibernate-type="java.lang.Long"></sql-type>
		<sql-type jdbc-type="INTEGER" hibernate-type="java.lang.Integer"></sql-type>
	</type-mapping>
	<table-filter match-catalog="qme" match-name="ANSWER_OPTION" />
	<table-filter match-catalog="qme" match-name="ANSWER_OPTION_MEDIA" />
	<table-filter match-catalog="qme" match-name="ANSWER_REFERENCE_MEDIA" />
	<table-filter match-catalog="qme" match-name="MEDIA_TYPE" />
	<table-filter match-catalog="qme" match-name="CATEGORY" />
	<table-filter match-catalog="qme" match-name="QUESTION" />
	<table-filter match-catalog="qme" match-name="QUESTION_HIT" />
	<table-filter match-catalog="qme" match-name="QUIZ" />
	<table-filter match-catalog="qme" match-name="QUIZ_QUESTION" />
	<table-filter match-catalog="qme" match-name="ROLE" />
	<table-filter match-catalog="qme" match-name="USER" />
	<table-filter match-catalog="qme" match-name="USER_CATEGORY" />
	<table-filter match-catalog="qme" match-name="USER_CATEGORY_LIKES" />
	<table-filter match-catalog="qme" match-name="USER_QUESTION_LIKES" />
	<table-filter match-catalog="qme" match-name="USER_QUIZ" />
	<table-filter match-catalog="qme" match-name="USER_QUIZ_GAME" />
	<table-filter match-catalog="qme" match-name="USER_QUIZ_LIKES" />
	<table-filter match-catalog="qme" match-name="USER_ROLES" />
	<table catalog="qme" name="QUESTION_HIT"
		   class="com.malcolm.qme.springdata.entity.QuestionHitEntity">
	</table>
	<table catalog="qme" name="ANSWER_OPTION_MEDIA"
		   class="com.malcolm.qme.springdata.entity.AnswerOptionMediaEntity">
	</table>
	<table catalog="qme" name="USER_QUIZ_GAME"
		   class="com.malcolm.qme.springdata.entity.UserQuizGameEntity">
	</table>
	<table catalog="qme" name="USER_QUIZ_LIKES"
		   class="com.malcolm.qme.springdata.entity.UserQuizLikesEntity">
	</table>
	<table catalog="qme" name="ANSWER_REFERENCE_MEDIA"
		   class="com.malcolm.qme.springdata.entity.AnswerReferenceMediaEntity">
	</table>
	<table catalog="qme" name="MEDIA_TYPE"
		   class="com.malcolm.qme.springdata.entity.MediaTypeEntity">
	</table>
	<table catalog="qme" name="QUIZ"
		   class="com.malcolm.qme.springdata.entity.QuizEntity">
	</table>
	<table catalog="qme" name="QUESTION"
		   class="com.malcolm.qme.springdata.entity.QuestionEntity">
	</table>
	<table catalog="qme" name="ROLE"
		   class="com.malcolm.qme.springdata.entity.RoleEntity">
	</table>
	<table catalog="qme" name="ANSWER_OPTION"
		   class="com.malcolm.qme.springdata.entity.AnswerOptionEntity">
	</table>
	<table catalog="qme" name="QUIZ_QUESTION"
		   class="com.malcolm.qme.springdata.entity.QuizQuestionEntity">
	</table>
	<table catalog="qme" name="USER_CATEGORY_LIKES"
		   class="com.malcolm.qme.springdata.entity.UserCategoryLikesEntity">
